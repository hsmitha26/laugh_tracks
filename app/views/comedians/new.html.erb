<h1>Add New Comedian</h1>

<section class="new-comedian-form">
  <%= form_for @comedian do |f| %>
    <%= f.label :name %>
    <%= f.text_field :name %>

    <%= f.label :age %>
    <%= f.number_field :age %>

    <%= f.label :city %>
    <%= f.text_field :city %>

    <%= f.submit %>
  <% end %>
</section>
